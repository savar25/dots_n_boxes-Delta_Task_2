<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.dots_n_boxes">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_new"
        android:label=" Dots \&apos;N\&apos; Boxes"
        android:roundIcon="@mipmap/ic_new_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".major"></activity>
        <activity android:name=".winner" />
        <activity
            android:name=".gamePage"
            android:screenOrientation="portrait" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>